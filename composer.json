{
    "name": "phaza/laravel-norwegian-zip-codes",
    "description": "Laravel models for Norwegian zip codes, municipalities and counties. Includes an artisan command to update data from the official authority.",
    "require": {
				"php": ">=5.5",
        "laravel/framework": "~5.1"

    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "orchestra/testbench": "~3.0",
				"mockery/mockery": "0.9.*"
    },
		"autoload": {
			"psr-4": {
				"NorwegianZipCodes\\": "src/"
			},
			"classmap": [
				"resources/NorwegianZipCodeSeeds.php"
			]
		},
		"autoload-dev": {
			"classmap": [
				"tests/",
				"resources/"
			]
		},
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Haza",
            "email": "peter.haza@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
